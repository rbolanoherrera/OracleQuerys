DECLARE 
   nuExiste number := 0;
BEGIN
   SELECT COUNT(*) INTO nuExiste FROM ALL_OBJECTS WHERE OBJECT_TYPE='PACKAGE' AND OBJECT_NAME='CG_CNESTADO_SALDO';
   IF (nuExiste = 1) THEN
      DBMS_OUTPUT.PUT_LINE('Se elimina paquete CG_CNESTADO_SALDO...');
      EXECUTE IMMEDIATE  'DROP PACKAGE CG_CNESTADO_SALDO';
   ELSE 
      DBMS_OUTPUT.PUT_LINE('No se encuentra el paquete CG_CNESTADO_SALDO...');
   END IF;
END;
